.app
  height 100%
  display flex
  flex-direction column
.app-header
  background-color aliceblue
  display flex
  height 80px
  flex 0 0 80px
  box-sizing border-box
  align-items center
  justify-content space-between
  padding 10px
  .image-load-btn
    label
      margin 0
      border 2px solid white
.content
  height calc(100% - 80px)
  display flex

